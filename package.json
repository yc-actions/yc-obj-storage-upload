{
  "name": "yc-actions-yc-obj-storage-upload",
  "version": "1.0.0",
  "description": "GitHub Action to upload files to Yandex Cloud Object Storage.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts --fix",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "GITHUB_WORKSPACE=__tests__ jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yc-actions/yc-sls-function.git"
  },
  "keywords": [
    "actions",
    "yandex cloud",
    "S3",
    "object storage",
    "upload"
  ],
  "author": "Nikolay Matrosov",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/lib-storage": "^3.485.0",
    "@grpc/grpc-js": "^1.9.13",
    "@smithy/protocol-http": "^3.0.12",
    "@yandex-cloud/nodejs-sdk": "^2.6.1",
    "glob": "^10.3.10",
    "mime-types": "^2.1.35",
    "minimatch": "9.0.1",
    "minimist": ">=1.2.8",
    "path-scurry": "^1.9.2"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.105",
    "@swc/jest": "^0.2.29",
    "@types/archiver": "^6.0.2",
    "@types/jest": "^29.5.11",
    "@types/mime-types": "^2.1.4",
    "@types/node": "20.11.2",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.18.0",
    "@vercel/ncc": "^0.38.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.56.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-github": "^4.10.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.1",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "prettier": "3.0.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  }
}
