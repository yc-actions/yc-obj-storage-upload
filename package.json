{
  "name": "yc-actions-yc-obj-storage-upload",
  "version": "1.0.0",
  "description": "GitHub Action to upload files to Yandex Cloud Object Storage.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts --fix",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "GITHUB_WORKSPACE=__tests__ jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yc-actions/yc-sls-function.git"
  },
  "keywords": [
    "actions",
    "yandex cloud",
    "S3",
    "object storage",
    "upload"
  ],
  "author": "Nikolay Matrosov",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@aws-sdk/client-s3": "^3.374.0",
    "@aws-sdk/lib-storage": "^3.374.0",
    "@grpc/grpc-js": "^1.8.17",
    "@smithy/protocol-http": "^1.1.1",
    "@yandex-cloud/nodejs-sdk": "^2.4.8",
    "glob": "^10.3.3",
    "mime-types": "^2.1.35",
    "minimatch": "9.0.1",
    "minimist": ">=1.2.8",
    "path-scurry": "^1.9.2"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.62",
    "@swc/jest": "^0.2.26",
    "@types/archiver": "^5.3.2",
    "@types/jest": "^29.5.3",
    "@types/mime-types": "^2.1.1",
    "@types/node": "20.4.4",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vercel/ncc": "^0.36.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.44.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-github": "^4.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.3",
    "jest": "^29.6.1",
    "js-yaml": "^4.1.0",
    "prettier": "2.8.8",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  }
}
